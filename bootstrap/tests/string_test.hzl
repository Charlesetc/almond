
# Test Tables

Describe "Strings" do

  Test "equals operator" do
    a = "test"
    b = "test"
    a == b
  end

  Test "not equals operator" do
    a = "test"
    b = "wow"
    not (a == b)
  end

end

Describe "Chars" do

  Test "not equals operator" do
    a = char a
    space = char ' '

    not (a == space)
  end

  Test "equals operator" do
    a = char 'a'
    b = char a

    a == b
  end

end
