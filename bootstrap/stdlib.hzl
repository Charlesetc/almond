
# Hazelnut Standard Library

binding (*) int do x int y int
  "a := *x * *y"
  "&a"
end

binding false bool do
  "a := false"
  "&a"
end

binding true bool do
  "a := true"
  "&a"
end

binding gets string do
  "
  reader := bufio.NewReader(os.Stdin)
  a, _ := reader.ReadString('\n')
  "
  "&a"
end

binding length int do x string
  "temp := len(*x)"
  "&temp"
end

binding nil bool do
  ""
  "into_any(NIL, nil)"
end
